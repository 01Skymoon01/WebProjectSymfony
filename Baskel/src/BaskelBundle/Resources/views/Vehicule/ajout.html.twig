
{% extends 'default/livreur/header.html.twig' %}



{% block head %}
    {{ parent() }}


{% endblock %}
{% block container %}









    <div class="single-product-tab-area mg-b-30">
        <!-- Single pro tab review Start-->
        <div class="single-pro-review-area">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="review-tab-pro-inner">
                            <ul id="myTab3" class="tab-review-design">
                                <li class="active"><a href="#"><i class="fa fa-truck" aria-hidden="true"></i> Ajout d'un vehicule</a></li>


                            </ul>

                            <div id="myTabContent" class="tab-content custom-product-edit">
                                <div class="product-tab-list tab-pane fade active in" id="description">
                                    {{ form_start(form) }}

                                    <div class="row">
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                            <div class="review-content-section">
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"><i class="icon nalika-credit-cards" aria-hidden="true"></i></span>
                                                    {{ form_widget(form.matricule, { 'attr': {'class': 'form-control','placeholder':'Matricule'} }) }}

                                                </div>


                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"><i class="icon nalika-user" aria-hidden="true"></i></span>
                                                    {{ form_widget(form.marque, { 'attr': {'class': 'form-control','placeholder':'Marque'} }) }}
                                                </div>



                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                            <div class="review-content-section">


                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"><i class="fa fa-truck"  aria-hidden="true"></i></span>
                                                    {{ form_widget(form.type, { 'attr': {'class': 'form-control pro-edt-select form-control-primary'} }) }}
                                                </div>


                                                <div class="row">
                                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                        <div class="text-center custom-pro-edt-ds">
                                                            <input type="submit" class="btn btn-ctl-bt waves-effect waves-light m-r-10" value="Ajouter" />
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                    {{ form_end(form) }}


                                </div>


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>









{% endblock %}





