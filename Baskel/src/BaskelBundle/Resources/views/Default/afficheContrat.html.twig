{% extends 'default/back/header.html.twig' %}



{% block head %}
    {{ parent() }}


{% endblock %}
{% block container %}

    <div class="breadcome-area" style="margin-left:14%"
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcome-list">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="icon nalika-home"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>Liste des Contrats</h2>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                            <div class="breadcomb-report">
                                <button data-toggle="tooltip" data-placement="left" title="Download Report" class="btn"><i class="icon nalika-download"></i></button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <div class="product-status mg-b-30" style="margin-left: 14%">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="product-status-wrap">
                        <h4>Liste des Contrats</h4>

                        <table>
                            <tr>
                                <th scope="col">&nbsp;Pack&nbsp;</th>
                                <th scope="col">&nbsp;Description&nbsp;</th>
                                <th scope="col">&nbsp; </th>
                                <th scope="col">&nbsp;&nbsp;</th>


                            </tr>
                            <tr>
                                {% for c in contrats %}
                            <tr>

                                <td>{{ c.pack }}&nbsp;</td>
                                <td>{{ c.description }}&nbsp;
                                <td>

                                    <a data-toggle="tooltip" class="pd-setting-ed" href="{{ path('SupprimerC', {'id': c.id}) }}"> <i class="fa fa-trash-o" aria-hidden="true"></i></a>
                                </td>
                                <td>

                                    <a data-toggle="tooltip" class="pd-setting-ed" href="{{ path('PDF', {'id': c.id}) }}"> <i class="fa fa-download" aria-hidden="true"></i></a>
                                </td>


                            </tr>
                            {% endfor %}


                            </tr>
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}




